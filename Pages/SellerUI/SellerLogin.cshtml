@page
@model EsyaStore.Pages.SellerUI.SellerLoginModel
@{
}



<div class="login">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-6">
                <div class="register-form">
                    <h2>Seller Login</h2>
                    <div class="row">
                        <form method="post">

                            <div class="col-md-6">
                                <label asp-for="tempSellerLogin.Email">Email</label>
                                <input asp-for="tempSellerLogin.Email" class="form-control">
                                <span asp-validation-for="tempSellerLogin.Email"></span>
                            </div>

                            <div class="col-md-6">
                                <label asp-for="tempSellerLogin.Password">Password</label>
                                <input asp-for="tempSellerLogin.Password" class="form-control">
                                <span asp-validation-for="tempSellerLogin.Password"></span>
                            </div>

                            <div class="col-md-12">
                                <button type="submit" class="btn">Submit</button>
                            </div>

                        </form>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>
